	File = {:Users/Ronen/Library/Application Support/Mendeley Desktop/Downloaded/Zou, Schatz - 2004 - Narrow plasmonicphotonic extinction and scattering line shapes for one and two dimensional silver nanoparticle arrays.pdf:pdf},
	Issn = {0021-9606},
	Journal = {The Journal of chemical physics},
	Month = dec,
	Number = {24},
	Pages = {12606--12},
	Pmid = {15606284},
	Title = {{Narrow plasmonic/photonic extinction and scattering line shapes for one and two dimensional silver nanoparticle arrays.}},
