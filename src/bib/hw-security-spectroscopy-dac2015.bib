	Journal = {Nano letters},
	Keywords = {Coupling,SEIRA,TCMT,and an immense body,and realization,coupling,critical coupling,dedicated to their design,eia,eit,essential element in the,fano resonance,in particular,nanophotonic toolkit,of work is,plasmonics,ptical resonators are an,seira,vibrational spectroscopy},
	Mendeley-Tags = {Coupling,SEIRA,TCMT},
	Month = may,
