@article{Fromm2004,
	Abstract = {Metallic "bowtie" nanoantennas consisting of two
opposing tip-to-tip Au triangles have been fabricated with triangle lengths of
75 nm and gaps ranging from 16 to 488 nm. For light polarized along the line
between the two triangles, the plasmon scattering resonance first blue-shifts
