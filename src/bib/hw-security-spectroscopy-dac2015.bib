@article{Shegai2011,
	Abstract = {Recent progress in nanophotonics includes
demonstrations of meta-materials displaying negative refraction at optical
frequencies, directional single photon sources, plasmonic analogies of
electromagnetically induced transparency and spectacular Fano resonances. The
physics behind these intriguing effects is to a large extent governed by the
